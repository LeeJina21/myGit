
public class A_ex03 {

	public static void main(String[] args) {
		int[] aaa = new int[100];
		
		for(int i=0; i<11; i++){
			aaa[i]=i;
			
			System.out.println(i);
			
		}
		System.out.println(aaa); /// ???? 얘 왜 이럼???
		
		
		
		for(int i=2; i<=9; i++) {
			for(int j=1; j<=9; j++) {
				System.out.println( i + " * " + j + " = " + i * j);
				System.out.println(); //구구단!
			}
		}
	}

}
